@model IEnumerable<restoran.Models.Kategori>

    <div class="wrap-label-gallery filter-tope-group size27 flex-w flex-sb-m m-l-r-auto flex-col-c-sm p-l-15 p-r-15 m-b-60">
        

        @foreach (var item in Model)
        {
            <a asp-action="Menu" class="label-gallery txt26 trans-0-4 @(ViewBag.CategoryId?.ToString()==item.Id.ToString()?"is-actived":"")" asp-route-id="@item.Id" >
               @item.Isim
            </a>
        }

    </div>

